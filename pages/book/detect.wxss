.tab {
    background-color: #fff;
    margin-top: 20rpx;
}

.tab-head {
    border-bottom: solid 1px #ddd;
    overflow: hidden;
    padding: 0 20rpx;
    font-size: 30rpx;
    background-color: #fff;
}

.swiper_container {
    height: 55rpx;
    width: 100%;
    background: #fafafa;
    position: relative;
    left: 0;
    top: 0;
    z-index: 12;
    border-bottom: 1px solid #ddd;
}

.swiper_container .fa {
    margin-left: 20rpx;
}

.swiper_item {
    margin-top: 10rpx;
    font-size: 25rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    letter-spacing: 2px;
}


.tab-head view {
    border-bottom: solid 2px transparent;
    float: left;
    padding: 10rpx 0;
    margin-right: 20rpx;
}

.tab-head .active {
    color: #007d40;
    border-bottom: solid 2px #007d40;
}

.tab-body {
    line-height: 160%;
}

.tab-body .right-item {
    display: none;
}

.tab-body .right-item.active {
    display: block;
}


.date {
    background-color: #ccc;
    color: #666;
    padding: 20rpx;
    font-size: 28rpx;
}

.dataitem {
    padding: 0 0 10rpx;
}

.timerbox {
    padding-top: 20rpx;
}

.timer {
    font-size: 32rpx;
    background-color: #fff;
    padding: 15rpx 20rpx;
    position: relative;
}

.timer .btn-book {
    vertical-align: middle;
    position: absolute;
    right: 20rpx;
    top: 15rpx;
    background-color: #007d40;
}

.num {
    color: #000;
    font-weight: bold;
}


.table {
    border: 1px solid darkgray;
    font-size: 28rpx;
    margin: 20px;
}

.tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 60rpx;
    align-items: center;
}

.td {
    width: 40%;
    justify-content: center;
    text-align: center;
}

.bg-w {
    background: snow;
}

.bg-g {
    background: #E6F3F9;
}

.th {
    width: 40%;
    justify-content: center;
    background: #007d40;
    color: #fff;
    display: flex;
    height: 60rpx;
    align-items: center;
}

.table-tit {
    padding: 0 20px;
    color: #007d40;
    font-size: 30rpx;
}

.text {
    font-size: 28rpx;
    padding: 0 30rpx;
    display: block;
}

.tip {
    font-size: 28rpx;
    padding: 0 30rpx;
    color: #666;
    display: block;
}


.step {
    padding: 10rpx 20rpx;
    margin: 0 30rpx 0;
    background: #007d40;
    color: #fff;
    font-size: 36rpx;
    text-align: center
}

.step_con {
    margin: 0 30rpx;
    border: 1rpx solid #ccc;
    text-align: center;
    padding: 10rpx;
    color: #007d40;
}

.step_tip {
    display: inline-block;
    text-align: center;
    font-size: 24rpx;
    color: #666;
}
.step_title{
    display: block;
}


.company {
    padding: 15rpx 0 0;
    overflow: hidden;
    background-color: #fff;
    border-bottom: 1px solid #ddd;
    position: relative
}

.company .pic {
    width: 200rpx;
    height: 200rpx;
    background-color: #eeeeee;
    position: absolute;
    left: 30rpx;
    top: 15rpx;
}

.comp_tit {
    font-size: 28rpx;
    margin-left: 260rpx;
    width: 400rpx;
}

.comp_desc {
    font-size: 24rpx;
    margin-left: 260rpx;
    color: #666;
    margin-top: 10rpx;
}

.comp_stars {
    margin-left: 240rpx;
    color: orange;
}

.comp_stars .fa {
    margin-right: 10rpx;
}

.comp_feature {
    margin-left: 260rpx;
    font-size: 24rpx;
    color: #666;
    margin-top: 10rpx;
}

.comp_feature text {
    margin-right: 10rpx;
    border: 1rpx solid #666;
    text-align: center;
    padding: 0 10rpx;
}

.region {
    background: rgba(0, 0, 0, .6);
    color: #fff;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
}

.info_desc {
    padding: 10rpx 30rpx 10rpx 0;
    font-size: 24rpx;
    background-color: #fff;
    clear: both;
    border-top: 2rpx solid #ddd;
    padding-top: 10rpx;
    margin-top: 35rpx;
    position: relative;
}

.info_desc .fa {
    margin-right: 10rpx;
}

.mapbox {
    width: 550rpx;
    line-height: 60rpx;
    padding-left: 30rpx;
    height: 60rpx;
}

.phoneboxi {
    position: absolute;
    right: 20rpx;
    top: 0;
    bottom: 0;
    font-size: 60rpx;
    border-left: 2rpx solid #ddd;
    padding-left: 30rpx;
    color: green;
}

.txt_j {
    position: absolute;
    left: 30rpx;
    top: 15rpx;
    background-color: #0069ee;
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.txt_j {
    position: absolute;
    left: 30rpx;
    top: 15rpx;
    background-color: #0069ee;
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.txt_l {
    position: absolute;
    left: 100rpx;
    top: 15rpx;
    background-color: green;
    text-align: center;
    color: #fff;
    font-size: 24rpx;
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.distance {
    color: #fff;
    font-size: 24rpx;
    position: absolute;
    right: 20rpx;
    top: 200rpx;
    background-color: #999;
    padding: 0 10rpx;
}


.tele {
    margin-left: 260rpx;
    font-size: 24rpx;
    background-color: #fff;
    margin-top: 15rpx;
}

.tele .fa {
    margin-right: 10rpx;
}

.right-item image {
    width: 100%;
    padding: 20rpx;
    box-sizing: border-box
}


.signHaibao[type=default] {
    width: 60rpx;
    font-weight: bold;
    padding: 2rpx;
    border: 4rpx solid green;
    color: green;
    position: absolute;
    right: 10rpx;
    top: 20rpx;
    font-size: 24rpx;
    text-align: center;
    display: block;
    border-radius: 0;
    box-sizing: content-box;
    line-height: 1.5;
}

.signHaibao .fa {
    font-size: 42rpx;
    color: #fd2727;
}

.signHaibao::after {
    border: none;
}

.signKefu {
    width: 100rpx;
    height: 50rpx;
    padding: 10rpx;
    background-color: #007d40;
    color: #fff;
    line-height: 50rpx;
    position: absolute;
    right: 140rpx;
    top: 0;
    font-size: 24rpx;
    text-align: center;
    display: none;
}

.mask_layer {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 999;
    left: 0;
    top: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
}

.modal_box {
    width: 76%;
    overflow: hidden;
    position: fixed;
    top: 50%;
    left: 0;
    z-index: 1001;
    background: #fafafa;
    margin: -150px 12% 0 12%;
    border-radius: 3px;
}

.ttle {
    padding: 15px;
    text-align: center;
    background-color: gazure;
}

.modalMsg {
    text-align: center;
    display: block;
    font-size: 28rpx;
    padding-bottom: 30rpx;
}
