/*  */
/*主容器*/
.stepper {
    text-indent: 0;
    width: 164rpx;
    height: 52rpx;
    /*给主容器设一个边框*/
    border: 2rpx solid #ccc;
    border-radius: 6rpx;
    position: absolute;
    right: 0;
    top: 0;
    display: inline-block;
    vertical-align: middle;
}

/*加号和减号*/
.stepper text {
    width: 40rpx;
    line-height: 52rpx;
    text-align: center;
    float: left;
}

/*数值*/
.stepper input {
    pointer-events: none;
    width: 80rpx;
    height: 52rpx;
    float: left;
    margin: 0 auto;
    text-align: center;
    font-size: 24rpx;
    max-height: auto;
    /*给中间的input设置左右边框即可*/
    border-left: 2rpx solid #ccc;
    border-right: 2rpx solid #ccc;
}

/*普通样式*/
.stepper .normal {
    color: black;
}

/*禁用样式*/
.stepper .disabled {
    color: #ccc;
}


